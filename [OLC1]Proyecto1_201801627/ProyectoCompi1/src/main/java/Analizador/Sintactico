package Analizador;
import java_cup.runtime.*;

parser code 
{:
    public String Resulto = "";   
:} 

terminal String IDENTIFICADOR, E_CADENA, DECI;
terminal String E_DG, E_GENREPEST, E_STR, E_DOUBLE;
terminal String E_GRALINE,E_ARCHIVO;
terminal String E_GRABAR,E_TITLE, E_EJEX;
terminal String LINECOMMENT;
terminal String E_EQUALS;
terminal String E_VALORES, E_TITLEX, E_TITLEY, E_GRAPIE;
terminal String E_KEYIZQ, E_KEYDER, E_CORIZQ, E_CORDER;
terminal String E_PYC, E_DOSP, E_COMA;


non terminal ini, contenidogeneral ,cuerpo, graficas, datosgraficabarra, datosgraficapie, datosgraficalinea, titulos, ejesx, contenidox, valoresbarras, contenidov, titulox, tituloy, archivo, comentario;

start with ini;

ini::=E_GENREPEST E_KEYIZQ contenidogeneral E_KEYDER {::};

contenidogeneral::=E_DG E_KEYIZQ cuerpo E_KEYDER{::}
                   |E_DG E_KEYIZQ cuerpo E_KEYDER graficas{::};

cuerpo::=cuerpo E_STR IDENTIFICADOR E_EQUALS E_CADENA E_PYC {::}
        |cuerpo E_DOUBLE IDENTIFICADOR E_EQUALS DECI E_PYC {::}
        |E_STR IDENTIFICADOR E_EQUALS E_CADENA E_PYC {::}
        |E_DOUBLE IDENTIFICADOR E_EQUALS DECI E_PYC {::}
;

graficas::=graficas E_GRABAR E_KEYIZQ datosgraficabarra E_KEYDER {::}
            |graficas E_GRAPIE E_KEYIZQ datosgraficapie E_KEYDER {::}
            |graficas E_GRALINE E_KEYIZQ datosgraficalinea E_KEYDER {::}
            |E_GRAPIE E_KEYIZQ datosgraficapie E_KEYDER {::}
            |E_GRALINE E_KEYIZQ datosgraficalinea E_KEYDER {::}
            |E_GRABAR E_KEYIZQ datosgraficabarra E_KEYDER {::}
;

datosgraficabarra::=datosgraficabarra titulos{::}
            |datosgraficabarra ejesx {::}
            |datosgraficabarra valoresbarras {::}
            |datosgraficabarra titulox {::}
            |datosgraficabarra tituloy {::}
            |titulos{::}
            |ejesx{::}
            |valoresbarras {::}
            |titulox {::}
            |tituloy {::}
;

datosgraficapie::=datosgraficapie titulos{::}
                |datosgraficapie ejesx{::}
                |datosgraficapie valoresbarras{::}
                |titulos{::}
                |ejesx{::}
                |valoresbarras{::}
;

datosgraficalinea::=datosgraficalinea titulos {::}
                  |datosgraficalinea archivo {::}
                  |titulos {::}
                  |archivo {::}
;

titulos::=E_TITLE E_DOSP IDENTIFICADOR E_PYC {::}
          |E_TITLE E_DOSP E_CADENA E_PYC {::}
;

ejesx::=E_EJEX E_DOSP E_CORIZQ contenidox E_CORDER E_PYC {::};

contenidox::=E_CADENA {::}
            |IDENTIFICADOR {::}
            |contenidox E_COMA {::}
            |contenidox E_CADENA {::}
            |contenidox IDENTIFICADOR {::}
;

valoresbarras::=E_VALORES E_DOSP E_CORIZQ contenidov E_CORDER E_PYC {::};

contenidov::=DECI {::}
            |IDENTIFICADOR {::}
            |E_CADENA {::}
            |contenidov E_COMA {::}
            |contenidov DECI {::}
            |contenidov E_CADENA {::}
            |contenidov IDENTIFICADOR {::}
;

titulox::=E_TITLEX E_DOSP IDENTIFICADOR E_PYC {::}
         |E_TITLEX E_DOSP E_CADENA E_PYC {::}
;

tituloy::=E_TITLEY E_DOSP IDENTIFICADOR E_PYC {::}
         |E_TITLEY E_DOSP E_CADENA E_PYC {::}
;

archivo::=E_ARCHIVO E_DOSP IDENTIFICADOR E_PYC {::}
      |E_ARCHIVO E_DOSP E_CADENA E_PYC {::}
;

comentario::=LINECOMMENT {::};