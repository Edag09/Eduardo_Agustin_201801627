operador::=E_AND{::}
        |E_OOR {::}
;

impresion::=E_CADENA {::}
           |IDENTIFICADOR {::}
           |numero {::}
           |IDENTIFiCADOR E_AUMENTO {::}
           |IDENTIFICADOR E_DISMINUIR {::}
