package AnalizadorJS;
import java_cup.runtime.*;

parser code 
{:
    public String Resulto = "";   
:} 

terminal String IDENTIFICADOR, E_CADENA, E_CADENAUC, DECI;
terminal String E_CLASS, E_CONSOLE, E_REQUIRE;
terminal String E_VAR, E_LET, E_CONST;
terminal String E_IF, E_ELSE, E_WHILE, E_DO, E_FOR, E_SWITCH, E_CASE, E_BREAK, E_DEFAULT;
terminal String E_PYC, E_PUNTO, E_DOSPUNTOS, E_COMA;
terminal String E_KEYIZQ, E_KEYDER, E_CORIZQ, E_CORDER, E_PARENTESISIZQ, E_PARENTESISDER;
terminal String E_MAS, E_MENOS, E_POR, E_DIVIDIR, E_POTENCIA, E_MODULO, E_EQUALS, E_AUMENTO, E_DISMINUIR;
terminal String E_COMPARAR, E_NOEQUALS, E_MENOR, E_MAYOR, E_MENORQUE, E_MAYORQUE;
terminal String E_AND, E_OOR, E_NEGAR;
terminal String E_TRUE, E_FALSE;

non terminal ini, procesos, parametros, variable, impresion, opmate, concatenar, condicion, forvariable, condicionfor, foraumento, casos, vector;

start with ini;

ini::=ini E_CLASS IDENTIFICADOR E_KEYIZQ procesos E_KEYDER {::}
    |ini IDENTIFICADOR E_PARENTESISIZQ E_PARENTESISDER E_KEYIZQ procesos E_KEYDER {::}
    |ini IDENTIFICADOR E_PARENTESISIZQ parametros E_PARENTESISDER E_KEYIZQ procesos E_KEYDER {::}
    |ini E_VAR IDENTIFICADOR E_EQUALS variable {::}
    |ini E_LET IDENTIFICADOR E_EQUALS variable {::}
    |ini E_CONST IDENTIFICADOR E_EQUALS variable {::}
    |ini E_VAR IDENTIFICADOR {::}
    |ini E_LET IDENTIFICADOR {::}
    |ini E_CONST IDENTIFICADOR {::}
    |ini E_VAR IDENTIFICADOR E_PYC {::}
    |ini E_LET IDENTIFICADOR E_PYC {::}
    |ini E_CONST IDENTIFICADOR E_PYC {::}
    |E_CLASS IDENTIFICADOR E_KEYIZQ procesos E_KEYDER {::}
    |IDENTIFICADOR E_PARENTESISIZQ E_PARENTESISDER E_KEYIZQ procesos E_KEYDER {::}
    |IDENTIFICADOR E_PARENTESISIZQ parametros E_PARENTESISDER E_KEYIZQ procesos E_KEYDER {::}
    |E_VAR IDENTIFICADOR E_EQUALS variable {::}
    |E_LET IDENTIFICADOR E_EQUALS variable {::}
    |E_CONST IDENTIFICADOR E_EQUALS variable {::}
    |E_VAR IDENTIFICADOR {::}
    |E_LET IDENTIFICADOR {::}
    |E_CONST IDENTIFICADOR {::}
    |E_VAR IDENTIFICADOR E_PYC {::}
    |E_LET IDENTIFICADOR E_PYC {::}
    |E_CONST IDENTIFICADOR E_PYC {::}
;

parametros::=IDENTIFICADOR {::}
          |parametros E_COMA IDENTIFICADOR {::}
          |parametros E_MAS opmate {::}
          |parametros E_MENOS opmate {::}
          |parametros E_POR opmate {::}
          |parametros E_DIVIDIR opmate {::}
          |parametros E_POTENCIA opmate {::}
;

variable::=E_CADENA E_PYC{::}
        |E_CADENA {::}
        |E_CADENAUC E_PYC {::}
        |E_CADENAUC {::}
        |DECI E_PYC{::}
        |DECI {::}
        |E_TRUE E_PYC{::}
        |E_TRUE {::}
        |E_FALSE E_PYC {::}
        |E_FALSE {::}
        |E_PARENTESISIZQ opmate E_PARENTESISDER opmate E_PYC {::}
        |E_PARENTESISIZQ opmate E_PARENTESISDER opmate {::}
        |E_PARENTESISIZQ opmate E_PARENTESISDER E_PYC{::}
        |E_PARENTESISIZQ opmate E_PARENTESISDER {::}
        |E_REQUIRE E_PARENTESISIZQ E_CADENA E_PARENTESISDER E_PYC {::}
        |E_REQUIRE E_PARENTESISIZQ E_CADENA E_PARENTESISDER {::}
        |E_CORIZQ vector E_CORDER {::}
        |E_CORIZQ vector E_CORDER E_PYC {::}
;

vector::=vector E_COMA E_CADENA {::}
       |vector E_COMA DECI {::}
       |vector E_COMA E_CORIZQ vector E_CORDER {::}
       |E_CADENA{::}
       |DECI {::}
       |E_CORIZQ vector E_CORDER {::}
;

procesos::=procesos IDENTIFICADOR E_PARENTESISIZQ E_PARENTESISDER E_KEYIZQ procesos E_KEYDER {::}
        |procesos IDENTIFICADOR E_PARENTESISIZQ parametros E_PARENTESISDER E_KEYIZQ procesos E_KEYDER {::}
        |procesos E_VAR IDENTIFICADOR E_EQUALS variable {::}
        |procesos E_LET IDENTIFICADOR E_EQUALS variable {::}
