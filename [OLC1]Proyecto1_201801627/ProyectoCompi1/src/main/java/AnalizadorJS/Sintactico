          |IDENTIFICADOR E_MENOR IDENTIFICADOR {::}
          |IDENTIFICADOR E_MAYORQUE IDENTIFICADOR {::}
          |IDENTIFICADOR E_MENORQUE IDENTIFICADOR {::}
          |IDENTIFICADOR E_COMPARAR IDENTIFICADOR {::}
          |DECI E_NOEQUALS DECI {::}
          |E_CADENA E_NOEQUALS E_CADENA {::}
