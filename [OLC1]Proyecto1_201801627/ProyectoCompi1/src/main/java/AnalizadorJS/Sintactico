          |E_PARENTESISIZQ condicion E_AND condicion E_PARENTESISDER {::}
;


casos::=casos E_CASE DECI E_DOSPUNTOS procesos E_BREAK E_PYC {::}             
       |casos E_DEFAULT E_DOSPUNTOS procesos E_BREAK E_PYC {::}
       |casos E_DEFAULT E_DOSPUNTOS procesos {::}
       |casos E_CASE E_CADENA E_DOSPUNTOS procesos E_BREAK E_PYC {::}
       |E_CASE DECI E_DOSPUNTOS procesos E_BREAK E_PYC {::}
       |E_DEFAULT E_DOSPUNTOS procesos E_BREAK E_PYC {::}
       |E_DEFAULT E_DOSPUNTOS procesos {::}
       |E_CASE E_CADENA E_DOSPUNTOS procesos E_BREAK E_PYC {::}
;

impresion::=impresion E_MAS opmate {::}
           |impresion E_MAS {::}
           |E_CADENA {::}
           |IDENTIFICADOR {::}
           |DECI {::}
           |E_MENOS DECI {::}
           |E_MENOS E_PARENTESISIZQ opmate E_PARENTESISDER {::}
           |E_PARENTESISIZQ opmate E_PARENTESISDER {::}
           |IDENTIFICADOR E_AUMENTO {::}
           |IDENTIFICADOR E_DISMINUIR {::}
           |E_NEGAR IDENTIFICADOR {::}
;

opmate::=opmate E_PARENTESISIZQ opmate E_PARENTESISDER {::}
        |opmate DECI E_POTENCIA DECI {::}
        |opmate DECI E_DIVIDIR DECI {::}
        |opmate DECI E_MODULO DECI {::}
        |opmate DECI E_POR DECI {::}
        |opmate DECI E_MAS DECI {::}
        |opmate DECI E_MENOS DECI {::}
        |E_PARENTESISIZQ opmate E_PARENTESISDER {::}
        |DECI E_POTENCIA DECI {::}
        |DECI E_DIVIDIR DECI {::}
        |DECI E_MODULO DECI {::}
        |DECI E_POR DECI {::}
        |DECI E_MAS DECI {::}
        |DECI E_MENOS DECI {::}
;